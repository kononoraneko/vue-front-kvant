"form-grid\" @submit.prevent=\"handleLogin\">\n        <label>\n          Логин\n          <input v-model.trim=\"authForm.login\" placeholder=\"user1\" autocomplete=\"username\">\n        </label>\n        <label>\n          Пароль\n          <input v-model=\"authForm.password\" type=\"password\" placeholder=\"••••••••\" autocomplete=\"current-password\">\n        </label>\n        <label>\n          Имя (для регистрации)\n          <input v-model.trim=\"authForm.name\" placeholder=\"Иван Иванов\" autocomplete=\"name\">\n        </label>\n        <div class=\"actions\">\n          <button type=\"submit\" :disabled=\"busy\">\n            {{ busy && action === 'login' ? 'Входим...' : 'Войти' }}\n          </button>\n          <button type=\"button\" class=\"secondary\" :disabled=\"busy\" @click=\"handleRegister\">\n            {{ busy && action === 'register' ? 'Создаём...' : 'Зарегистрироваться' }}\n          </button>\n        </div>\n      </form>\n      <p class=\"hint\">После входа ID пользователя сохранится и станет доступен профиль и курсы.</p>\n    </section>\n\n    <section v-if=\"user\" class=\"card\">\n      <div class=\"section-header\">\n        <h2>2. Профиль пользователя</h2>\n        <div>\n          <span class=\"tag\">ID: {{ user.id }}</span>\n          <span class=\"tag\">{{ user.role || 'Role?' }}</span>\n        </div>\n      </div>\n      <p class=\"user-name\">{{ user.name }}</p>\n      <div class=\"actions inline\">\n        <button type=\"button\" :disabled=\"busy\" @click=\"loadProfile\">\n          {{ busy && action === 'profile' ? 'Обновляем...' : 'Обновить профиль' }}\n        </button>\n        <button type=\"button\" class=\"ghost\" @click=\"resetUser\">Сбросить</button>\n      </div>\n\n      <div v-if=\"profile\" class=\"columns\">\n        <div>\n          <h3>Созданные курсы</h3>\n          <ul>\n            <li v-for=\"course in profile.created_courses\" :key=\"course.id\">\n              <div>\n                <strong>{{ course.title }}</strong>\n                <span class=\"tag\">{{ course.status }}</span>\n              </div>\n              <button type=\"button\" @click=\"loadContent(course)\">Контент</button>\n            </li>\n            <li v-if=\"!profile.created_courses.length\" class=\"empty\">Нет курсов</li>\n          </ul>\n        </div>\n        <div>\n          <h3>Доступные курсы</h3>\n          <ul>\n            <li v-for=\"course in profile.enrolled_courses\" :key=\"course.id\">\n              <div>\n                <strong>{{ course.title }}</strong>\n                <span class=\"tag\">{{ course.status }}</span>\n              </div>\n              <button type=\"button\" @click=\"loadContent(course)\">Контент</button>\n            </li>\n            <li v-if=\"!profile.enrolled_courses.length\" class=\"empty\">Пока пусто</li>\n          </ul>\n        </div>\n      </div>\n    </section>\n\n    <section v-if=\"courseContent\" class=\"card\">\n      <div class=\"section-header\">\n        <h2>3. Содержимое курса</h2>\n        <span class=\"tag\">курс #{{ selectedCourse?.id }} - {{ selectedCourse?.title }}</span>\n      </div>\n      <pre>{{ formattedContent }}</pre>\n    </section>\n\n    <p v-if=\"notification.text\" class=\"notice\" :class=\"notification.type\">\n      {{ notification.text }}\n    </p>\n  </div>\n</template>\n\n<script setup>\nimport { computed, reactive, ref } from 'vue'\n\nconst apiUrl = process.env.VUE_APP_API_URL || 'http://localhost:8000'\n\nconst authForm = reactive({\n  login: '',\n  password: '',\n  name: ''\n})\n\nconst user = ref(null)\nconst profile = ref(null)\nconst selectedCourse = ref(null)\nconst courseContent = ref(null)\nconst notification = reactive({ type: 'info', text: '' })\nconst busy = ref(false)\nconst action = ref(null)\n\nconst formattedContent = computed(() =>\n  courseContent.value ? JSON.stringify(courseContent.value, null, 2) : ''\n)\n\nfunction setNotice(type, text) {\n  notification.type = type\n  notification.text = text\n}\n\nfunction resetUser() {\n  user.value = null\n  profile.value = null\n  selectedCourse.value = null\n  courseContent.value = null\n  setNotice('info', 'Пользователь сброшен. Введите данные заново.')\n}\n\nasync function callApi(path, options) {\n  const url = `${apiUrl}${path}`\n  const response = await fetch(url, options)\n  if (!response.ok) {\n    const detail = await response.json().catch(() => ({}))\n    throw new Error(detail.detail || response.statusText)\n  }\n  return response.json()\n}\n\nasync function handleLogin() {\n  if (!authForm.login || !authForm.password) {\n    setNotice('error', 'Логин и пароль обязательны.')\n    return\n  }\n  busy.value = true\n  action.value = 'login'\n  try {\n    const params = new URLSearchParams({\n      login: authForm.login,\n      password: authForm.password\n    })\n    const data = await callApi(`/auth/login?${params.toString()}`)\n    user.value = data\n    setNotice('success', 'Успешно вошли.')\n    await loadProfile()\n  } catch (error) {\n    setNotice('error', error.message)\n  } finally {\n    busy.value = false\n    action.value = null\n  }\n}\n\nasync function handleRegister() {\n  if (!authForm.login || !authForm.password || !authForm.name) {\n    setNotice('error', 'Для регистрации нужны логин, пароль и имя.')\n    return\n  }\n  busy.value = true\n  action.value = 'register'\n  try {\n    const data = await callApi('/auth/register', {\n      method: 'POST',\n      headers: { 'Content-Type': 'application/json' },\n      body: JSON.stringify({\n        login: authForm.login,\n        password: authForm.password,\n        name: authForm.name\n      })\n    })\n    user.value = data\n    setNotice('success', 'Пользователь создан. Можно обновить профиль.')\n    await loadProfile()\n  } catch (error) {\n    setNotice('error', error.message)\n  } finally {\n    busy.value = false\n    action.value = null\n  }\n}\n\nasync function loadProfile() {\n  if (!user.value?.id) {\n    setNotice('error', 'Не найден ID пользователя.')\n    return\n  }\n  busy.value = true\n  action.value = 'profile'\n  try {\n    profile.value = await callApi(`/profile?user_id=${user.value.id}`)\n    setNotice('success', 'Профиль обновлен.')\n  } catch (error) {\n    setNotice('error', error.message)\n  } finally {\n    busy.value = false\n    action.value = null\n  }\n}\n\nasync function loadContent(course) {\n  if (!user.value?.id) {\n    setNotice('error', 'Сначала войдите.')\n    return\n  }\n  busy.value = true\n  action.value = 'content'\n  try {\n    selectedCourse.value = course\n    const data = await callApi(`/courses/${course.id}/content?user_id=${user.value.id}`)\n    courseContent.value = data.content\n    setNotice('success', `Загружен курс \"${course.title}\".`)\n  } catch (error) {\n    setNotice('error', error.message)\n  } finally {\n    busy.value = false\n    action.value = null\n  }\n}\n</script>\n\n<style>\n:root {\n  color-scheme: light;\n}\n\n#app {\n  font-family: 'Segoe UI', Arial, sans-serif;\n  background: #f6f8fa;\n  min-height: 100vh;\n  padding: 24px;\n  color: #1f2328;\n}\n\n.app {\n  max-width: 960px;\n  margin: 0 auto;\n  display: flex;\n  flex-direction: column;\n  gap: 20px;\n}\n\n.hero {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  background: #fff;\n  padding: 20px;\n  border-radius: 10px;\n  box-shadow: 0 10px 30px rgba(15, 23, 42, 0.08);\n}\n\n.card {\n  background: #fff;\n  padding: 20px;\n  border-radius: 12px;\n  box-shadow: 0 10px 30px rgba(15, 23, 42, 0.08);\n}\n\n.form-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(220px, 1fr));\n  gap: 16px;\n}\n\nlabel {\n  display: flex;\n  flex-direction: column;\n  gap: 6px;\n  font-weight: 600;\n}\n\ninput {\n  border: 1px solid #d0d7de;\n  border-radius: 6px;\n  padding: 8px 10px;\n  font-size: 15px;\n}\n\n.actions {\n  display: flex;\n  gap: 12px;\n  align-items: flex-end;\n}\n\n.actions.inline {\n  margin-top: 12px;\n}\n\nbutton {\n  border: none;\n  border-radius: 6px;\n  padding: 10px 16px;\n  font-weight: 600;\n  cursor: pointer;\n  background: #2563eb;\n  color: #fff;\n  transition: opacity 0.2s ease;\n}\n\nbutton:disabled {\n  opacity: 0.6;\n  cursor: not-allowed;\n}\n\n.secondary {\n  background: #22c55e;\n}\n\n.ghost {\n  background: transparent;\n  color: #2563eb;\n  border: 1px solid #2563eb;\n}\n\n.section-header {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  gap: 16px;\n}\n\n.tag {\n  display: inline-flex;\n  padding: 4px 8px;\n  border-radius: 999px;\n  background: #eff3f6;\n  font-size: 12px;\n  font-weight: 600;\n}\n\n.user-name {\n  font-size: 22px;\n  font-weight: 700;\n  margin: 8px 0 4px;\n}\n\n.columns {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(240px, 1fr));\n  gap: 20px;\n  margin-top: 20px;\n}\n\nul {\n  list-style: none;\n  padding: 0;\n  display: flex;\n  flex-direction: column;\n  gap: 10px;\n}\n\nli {\n  border: 1px solid #d0d7de;\n  border-radius: 10px;\n  padding: 10px;\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  gap: 12px;\n}\n\n.empty {\n  border-style: dashed;\n  color: #6e7781;\n  font-style: italic;\n}\n\n.hint {\n  margin-top: 12px;\n  color: #6e7781;\n}\n\npre {\n  background: #0f172a;\n  color: #f8fafc;\n  padding: 16px;\n  border-radius: 10px;\n  overflow: auto;\n  font-size: 14px;\n  max-height: 400px;\n}\n\n.notice {\n  padding: 12px 16px;\n  border-radius: 8px;\n  font-weight: 600;\n  text-align: center;\n}\n\n.notice.success {\n  background: #dcfce7;\n  color: #14532d;\n}\n\n.notice.error {\n  background: #fee2e2;\n  color: #7f1d1d;\n}\n\n.notice.info {\n  background: #dbeafe;\n  color: #1e3a8a;\n}\n\n@media (max-width: 640px) {\n  .hero {\n    flex-direction: column;\n    align-items: flex-start;\n    gap: 12px;\n  }\n\n  .actions {\n    flex-direction: column;\n    align-items: stretch;\n  }\n\n  li {\n    flex-direction: column;\n    align-items: flex-start;\n  }\
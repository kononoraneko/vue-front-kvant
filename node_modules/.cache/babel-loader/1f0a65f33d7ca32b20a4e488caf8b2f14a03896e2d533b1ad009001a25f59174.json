{"ast":null,"code":"import \"core-js/modules/es.json.stringify.js\";\nimport \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport { computed, reactive, ref } from 'vue';\nexport default {\n  __name: 'App',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const apiUrl = process.env.VUE_APP_API_URL || 'http://localhost:8000';\n    const authForm = reactive({\n      login: '',\n      password: '',\n      name: ''\n    });\n    const user = ref(null);\n    const profile = ref(null);\n    const selectedCourse = ref(null);\n    const courseContent = ref(null);\n    const notification = reactive({\n      type: 'info',\n      text: ''\n    });\n    const busy = ref(false);\n    const action = ref(null);\n    const formattedContent = computed(() => courseContent.value ? JSON.stringify(courseContent.value, null, 2) : '');\n    function setNotice(type, text) {\n      notification.type = type;\n      notification.text = text;\n    }\n    function resetUser() {\n      user.value = null;\n      profile.value = null;\n      selectedCourse.value = null;\n      courseContent.value = null;\n      setNotice('info', 'Пользователь сброшен. Введите данные заново.');\n    }\n    async function callApi(path, options) {\n      const url = `${apiUrl}${path}`;\n      const response = await fetch(url, options);\n      if (!response.ok) {\n        const detail = await response.json().catch(() => ({}));\n        throw new Error(detail.detail || response.statusText);\n      }\n      return response.json();\n    }\n    async function handleLogin() {\n      if (!authForm.login || !authForm.password) {\n        setNotice('error', 'Логин и пароль обязательны.');\n        return;\n      }\n      busy.value = true;\n      action.value = 'login';\n      try {\n        const params = new URLSearchParams({\n          login: authForm.login,\n          password: